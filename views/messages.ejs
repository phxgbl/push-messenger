<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Staffs</title>
</head>

<body>
    <table>
        <tr>
            <th>Title</th>
            <th>Body</th>
            <th>CreatedAt</th>
        </tr>
        <% if(messages.length>0)%>
        <% messages.forEach(function(message){ %>
            <tr>
                <td><%=message.title%></td>
                <td><%=message.body%></td>
                <td><%=message.createdAt%></td>
            </tr>
        <% }); %>


    </table>
</body>

</html>